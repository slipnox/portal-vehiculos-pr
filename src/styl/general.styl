.pv-container
  background #f1f1f1
  max-width $siteWith
  margin 150px auto

.section
  padding 0

.section__title
  max-width  100%
  background #d8d8d8
  padding 5px 15px

.section__content
  padding 15px 8%

.section__field
  display flex
  flex-flow row
  align-items center
  margin 0 0 10px 0

  .input-name
    font-size 13px
    font-weight 500

  .select,
  .vertical-inputs,
  .input-group,
  .input
    width 100%
    max-width 195px

  > *
    flex 1
    margin 0

.table-title
  font-weight 600

.section__edit-btn
  max-width 95px
  margin-right 100px

.transfer-via .section__field .select,
.transfer-via .section__field .vertical-inputs,
.transfer-via .section__field .input
  min-width 195px

.transfer-includes .section__field .select
  //max-width 80px

.section__field.align-items-top
  align-items flex-start

.vertical-inputs
  padding-top 3px

.left-column
  display: flex
  flex-direction column
  justify-content flex-start

  > *
    width: 100%

.margin-top-auto
  margin auto 0 0 0

.horizontal-inputs
  display flex
  align-items baseline
  max-width 195px
  justify-content flex-start

.submit-form
  text-align center

  button
    width 150px
    margin 15px 0 30px 0

.calendar-icon
  max-height 28px
  padding 3px 5px
  width 40px
  height 40px
  background transparent url("data:image/svg+xml;base64,") no-repeat
  background-size 18px
  background-position center

.section__detail-box
  flex 2
  padding 15px
  background #fff
  border solid 1px #d8d8d8

  .section__field
    margin-bottom 4px


.client-info .right-column .section__detail-box
  flex 1
  width 195px
  padding 5px

.client-info .right-column .section__field:last-child
  padding-top 10px

.client-info .right-column .section__detail-box textarea
  width 100%

.open-document:focus,
.open-document:hover,
.open-document,
.find-licence:focus,
.find-licence:hover,
.find-licence
  display block
  margin-right -28px
  width 40px
  flex 0
  border none
  background transparent url("data:image/svg+xml;base64,") no-repeat
  background-size 18px
  background-position center
  z-index 10

#zip-address,
#client-address
  font-size 12px

.plane-input
  border none
  border-bottom solid 1px #ddd

.boxes
  display: grid
  grid-gap: 1rem
  grid-template-columns: repeat(auto-fill,minmax(15rem,1fr))


.transaction-info-table
  max-width 75%
  margin 0 auto

.f6 .transaction-info .section__field
  margin-bottom 6px

.f6 .transaction-info .section__field .select,
.f6 .transaction-info .section__field .vertical-inputs,
.f6 .transaction-info .section__field .input
  min-width 220px

.open-document:focus,
.open-document:hover,
.open-document
  width 20px
  height 18px
  padding 0
  transform scaleX(-1)
  margin 0 auto

.f6 .submit-form
  text-align right

//General Tables
.pv-table
  margin 0 auto
  display table
  width 100%
  font-size 12px
  border 1px solid #d8d8d8

.pv-table-row
  display table-row

.pv-table-heading
  display table-header-group
  font-weight bold
  font-size: 11px;

.pv-table-cell, .pv-table-head
  display table-cell
  padding 8px 10px

.pv-table-heading .pv-table-cell:not(:last-child),
.pv-table-head:not(:last-child),
.pv-table-cell:not(:last-child)
  border-right 1px solid #d8d8d8

.pv-table-head,
.pv-table-heading .pv-table-cell,
.pv-table-row:not(:last-child) .pv-table-cell
  border-bottom 1px solid #d8d8d8

.pv-table-body
  display table-row-group

.pv-table-foot .pv-table-cell
  border-top solid 1px #d8d8d8
  background transparent

.pv-table-foot
  font-size 14px
  font-weight bold

.pv-table-foot .pv-table-cell
  border-right none

.transaction-rights-info .pv-table
  width 60%

.current-assessments .pv-table
  width 80%

.fines-info .pv-table
  width 75%

//CUSTOM TABLES

// Details Table
.pv-table.pv-details-table,
.pv-table.pv-details-table .pv-table-cell
  border:none

.pv-details-table .pv-table-cell,
.pv-details-table .pv-table-head
  padding 8px 10px 8px 0

.pv-table.pv-details-table .pv-table-cell
  width 25%

.pv-table.pv-details-table .pv-table-cell:nth-child(odd)
  font-weight bold

// Zebra Table
.pv-zebra-table .pv-table-cell:not(:first-child),
.pv-zebra-table .pv-table-heading .pv-table-head
  text-align: center;

.pv-table.pv-zebra-table
  .pv-table-head
    padding 6px 8px

.pv-table.pv-zebra-table
  .pv-table-cell
    padding 10px 8px

.pv-table.pv-zebra-table
  .pv-table-row:nth-child(even)
    background #fff

.pv-table.pv-zebra-table
  .pv-table-row:nth-child(odd)
    background #f8f8f8

.pv-table.pv-zebra-table
  .pv-table-foot
    .pv-table-row:nth-child(odd)
      background transparent

.pv-table.pv-zebra-table
  .pv-table-head
    background rgb(243,243,243)
    background -moz-linear-gradient(top, rgba(243,243,243,1) 0%, rgba(255,255,255,1) 100%)
    background -webkit-linear-gradient(top, rgba(243,243,243,1) 0%,rgba(255,255,255,1) 100%)
    background linear-gradient(to bottom, rgba(243,243,243,1) 0%,rgba(255,255,255,1) 100%)

.pv-table.pv-zebra-table
  .pv-table-foot
    border-top solid 1px #d8d8d8
    display: table-row-group
    background transparent

.pv-table.pv-zebra-table
  .pv-table-heading
    .pv-table-cell
      border-bottom none

.pv-table.pv-zebra-table
  .pv-table-row:not(:last-child)
    .pv-table-cell
      border-bottom none

// Zebra Table Narrow
.pv-table.pv-zebra-table.narrow
  .pv-table-head,
  .pv-table-cell
    padding 4px 8px

.pv-table.pv-zebra-table.narrow
  .pv-table-heading
    .pv-table-cell
      padding: 2px 2px

.pv-table.pv-zebra-table.narrow.documents
  .pv-table-body
    .pv-table-cell:last-child
      padding 2px 0 0

.transaction-rights-info
  .pv-table
    .pv-table-cell:last-child
      text-align right
