.pv-container
  background #f1f1f1
  max-width $siteWith
  margin 150px auto

.section
  padding 0

.section__title
  max-width  100%
  background #d8d8d8
  padding 5px 15px

.section__content
  padding 30px 8%

.section__field
  display flex
  flex-flow row
  align-items center
  margin 0 0 10px 0

  .input-name
    font-size 13px
    font-weight 500

  .select,
  .vertical-inputs,
  .input-group,
  .input
    width 100%
    max-width 195px

  > *
    flex 1
    margin 0

.table-title
  font-weight 600

.section__edit-btn
  max-width 95px
  margin-right 100px

.transfer-via .section__field .select,
.transfer-via .section__field .vertical-inputs,
.transfer-via .section__field .input
  min-width 195px

.transfer-includes .section__field .select
  //max-width 80px

.section__field.align-items-top
  align-items flex-start

.vertical-inputs
  padding-top 3px

.left-column
  display: flex
  flex-direction column
  justify-content flex-start

  > *
    width: 100%

.margin-top-auto
  margin auto 0 0 0

.horizontal-inputs
  display flex
  align-items baseline
  max-width 195px
  justify-content flex-start

.submit-form
  text-align center

  button
    width 150px
    margin 15px 0 30px 0

.calendar-icon
  max-height: 28px
  padding: 3px 5px;
  width: 40px;
  height: 40px;
  background transparent url("data:image/svg+xml;base64,") no-repeat
  background-size 18px
  background-position center

.section__detail-box
  flex 2
  padding 15px
  background #fff
  border solid 1px #d8d8d8

  .section__field
    margin-bottom 4px


.client-info .right-column .section__detail-box
  flex 1
  width 195px
  padding 5px

.client-info .right-column .section__field:last-child
  padding-top 10px

.client-info .right-column .section__detail-box textarea
  width 100%

.find-licence:focus,
.find-licence:hover,
.find-licence
  display block
  margin-right -28px
  width 40px
  flex 0
  border none
  background transparent url("data:image/svg+xml;base64,") no-repeat
  background-size 18px
  background-position center
  z-index 10

#zip-address,
#client-address
  font-size 12px

.plane-input
  border none
  border-bottom solid 1px #ddd

.boxes
  display: grid
  grid-gap: 1rem
  grid-template-columns: repeat(auto-fill,minmax(15rem,1fr))


.transaction-info-table,
.transaction-info-table td,
.transaction-info-table th,
.details-info-table,
.details-info-table td,
.details-info-table th
  background transparent
  border none

.transaction-info-table td,
.transaction-info-table th,
.info-table-gray td,
.info-table-gray th,
.details-info-table td
  width 25%
  padding 5px 0
  font-size 12px

.details-info-table td
  vertical-align top

.transaction-info-table
  max-width 75%
  margin 0 auto

.transaction-info-table td,
.transaction-info-table th,
.info-table-gray td,
.info-table-gray th
  padding 8px

.transaction-info-table > tbody th,
.info-table-gray th
  font-weight:500

.transaction-info-table th,
.transaction-info-table td,
.info-table-gray td
  text-align center

.transaction-info-table > tbody td,
.transaction-info-table > tbody th
  background #fff
  border solid 1px #d8d8d8

.transaction-info-table > tbody th
  background #f8f9fa

.transaction-info-table tfoot
  font-weight 500

.transaction-info-table td.transaction-total
  text-align right
  text-transform uppercase
  line-height 24px

.f6 .transaction-info .section__field
  margin-bottom 6px

.f6 .transaction-info .section__field .select,
.f6 .transaction-info .section__field .vertical-inputs,
.f6 .transaction-info .section__field .input
  min-width 220px

.current-assessments-table
  max-width 70%
  margin 0 auto

.fines-table
  width 85%
  margin 0 auto

.fines-table th,
.fines-table td
  width 17.5%

.fines-table th
  text-align center

.fines-table .fine-description
  text-align left
  width 30%

.f6 .transaction-info-table
  max-width 100%

.f6 .transaction-info-table td,
.f6 .transaction-info-table th
  width 10%

.f6 .transaction-info-table .end-column,
.f6 .transaction-info-table .start-column
  width 20%

.f6 .transaction-info-table .center-column
  width 15%
